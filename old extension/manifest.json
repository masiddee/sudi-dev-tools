{
    "manifest_version": 2,
    
    "name": "SUDI DEV TOOLS EXPERIMENTAL",
    "description": "This is Sudi's test dev extension",
    "version": "0.1",
    "devtools_page": "devtools.html",
    
    "background": {
        "scripts": ["jquery-2.2.0.min.js","eventPage.js"],
        "persistent": false
    },
    
    "browser_action": {
        "default_icon": "icon.png",
        // "default_popup": "popup.html",
        "default_title": "Vote for Sudi!"
    },
    
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["jquery-2.2.0.min.js","content_script.js"],
            // "css": ["panel.css"],
            "run_at": "document_end",
            "all_frames": true
        }
    ],
    
    "permissions": [
        "tabs",
        "cookies",
        // "webRequest",
        "<all_urls>"
        // "*://*.pardot.com"
        // "https://ajax.googleapis.com/"
    ]
}